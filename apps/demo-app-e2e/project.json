{
  "name": "demo-app-e2e",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/demo-app-e2e/src",
  "projectType": "application",
  "targets": {
    "e2e": {
        "executor": "nx:run-commands",
        "options": {
          "commands": [
            "nx build demo-app",
             "start-server-and-test --expect 200 'http-server -c-1 ./dist/apps/demo-app' http://127.0.0.1:8080 'nx _e2e demo-app-e2e'"
          ]
        }
    },
    "_e2e": {
      "executor": "@nrwl/cypress:cypress",
      "options": {
        "cypressConfig": "apps/demo-app-e2e/cypress.config.ts",
        "baseUrl": "http://localhost:8080",
        "testingType": "e2e"
      },
      "configurations": {
        "production": {
          "devServerTarget": "demo-app:serve:production"
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/demo-app-e2e/**/*.{js,ts}"]
      }
    }
  },
  "tags": [],
  "implicitDependencies": ["demo-app"]
}
